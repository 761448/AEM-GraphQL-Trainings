kages/aem-guides-wknd-graphql/basic-tutorial/src/components/Home.js",[],[],"/Users/macintosh/AEM/aem-custom-packages/aem-guides-wknd-graphql/basic-tutorial/src/components/Person.js",[],[],"/Users/macintosh/AEM/aem-custom-packages/aem-guides-wknd-graphql/basic-tutorial/src/components/Teams.js",["59"],[],"/*\nCopyright 2022 Adobe\nAll Rights Reserved.\n\nNOTICE: Adobe permits you to use, modify, and distribute this file in\naccordance with the terms of the Adobe license agreement accompanying\nit.\n*/\n\nimport React from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { useAllTeams } from \"../api/usePersistedQueries\";\nimport Error from \"./Error\";\nimport Loading from \"./Loading\";\nimport \"./Teams.scss\";\n\n\nfunction Teams() {\n  \n  //*********************************\n  // TODO :: Implement this by following the steps from AEM Headless Tutorial ==> Implement Teams functionality\n  // https://experienceleague.adobe.com/docs/experience-manager-learn/getting-started-with-aem-headless/graphql/multi-step/graphql-and-react-app.html#implement-teams-functionality\n  //*********************************\n  // Get the Teams data from AEM using the useAllTeams \n  const { teams, error } = useAllTeams();\n\n  // Handle error and loading conditions\n  if (error) {\n    // If an error ocurred while executing the GraphQL query, display an error message\n    return <Error errorMessage={error} />;\n  } else if (!teams) {\n    // While the GraphQL request is executing, show the Loading indicator\n    return <Loading />;\n  }\n  // Teams have been populated by AEM GraphQL query. Display the teams.\n  return (\n    <div className=\"teams\">\n      {teams.map((team, index) => {\n        return <Team key={index} {...team} />;\n      })}\n    </div>\n  );\n}\n\n// Render single Team\nfunction Team({ title, shortName, description, teamMembers, teamPicture }) {\n  // Must have title, shortName and at least 1 team member\n  if (!title || !shortName || !teamMembers) {\n    return null;\n  }\n\n  return (\n    <div className=\"team\">\n      <img\n        className=\"team__image\"\n        src={process.env.REACT_APP_HOST_URI+teamPicture._path}\n      />\n      <h2 className=\"team__title\">{title}</h2>\n      <p className=\"team__description\">{description.plaintext}</p>\n      <div>\n        <h4 className=\"team__members-title\">Members</h4>\n        <ul className=\"team__members\">\n          {/* Render the referenced Person models associated with the team */}\n          {teamMembers.map((teamMember, index) => {\n            return (\n              <li key={index} className=\"team__member\">\n                <Link to={`/person/${teamMember.fullName}`}>\n                  {teamMember.fullName}\n                </Link>\n              </li>\n            );\n          })}\n        </ul>\n      </div>\n    </div>\n  );\n}\n\n\nexport default Teams;\n","/Users/macintosh/AEM/aem-custom-packages/aem-guides-wknd-graphql/basic-tutorial/src/api/usePersistedQueries.js",[],[],"/Users/macintosh/AEM/aem-custom-packages/aem-guides-wknd-graphql/basic-tutorial/src/api/aemHeadlessClient.js",[],[],"/Users/macintosh/AEM/aem-custom-packages/aem-guides-wknd-graphql/basic-tutorial/src/components/Error.js",[],[],"/Users/macintosh/AEM/aem-custom-packages/aem-guides-wknd-graphql/basic-tutorial/src/components/Loading.js",[],[],"/Users/macintosh/AEM/aem-custom-packages/aem-guides-wknd-graphql/basic-tutorial/src/utils/renderRichText.js",[],[],"/Users/macintosh/AEM/aem-custom-packages/aem-guides-wknd-graphql/basic-tutorial/src/components/Match.js",[],[],{"ruleId":"60","severity":1,"message":"61","line":15,"column":8,"nodeType":"62","messageId":"63","endLine":15,"endColumn":12},{"ruleId":"64","severity":1,"message":"65","line":54,"column":7,"nodeType":"66","endLine":57,"endColumn":9},"no-unused-vars","'Team' is defined but never used.","Identifier","unusedVar","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty str